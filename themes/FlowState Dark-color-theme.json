	"semanticHighlighting": true,
	"semanticTokenColors": {
		"*.declaration": "#faadf5",
		"class": "#c6a2ff",
		"comment": "#505050",
		"enum": "#c6a2ff",
		"enumMember": "#dbc8fa",
